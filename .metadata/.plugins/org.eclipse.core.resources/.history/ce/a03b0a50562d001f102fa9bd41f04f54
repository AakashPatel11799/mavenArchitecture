		ClassPathXmlApplicationContext context=new ClassPathXmlApplicationContext("info.xml");
		Student s1=context.getBean("s1",Student.class);
		Address a1=context.getBean("a1",Address.class);
		s1.setAddress(a1);
		System.out.println(s1);
	}

}
