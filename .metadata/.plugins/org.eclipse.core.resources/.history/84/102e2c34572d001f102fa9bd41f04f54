              <property name="state" value="mp"/>
        </bean>
        -->
        <bean id="s1" class="com.db.Student1">
        <property name="id" value="101"/>
        <property name="name" value="ram"/>
        <property name="address" ref="a1"/>
        <property name="email">
           <list>
           <value>aaksh@gmail.com</value>
           <value>milan@gmail.com</value>
           </list>
        
        </property>
        </bean>
        
